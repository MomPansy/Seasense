  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^20.19.24",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "wait-on": "^9.0.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "envx": "dotenvx",
    "envx:run": "npm run envx -- run --env-file .env.production --env-file .env.development --overload",
    "envx:encrypt": "dotenvx encrypt -f .env.production",
    "dev:db": "docker compose up -d",
    "dev:db:wait": "npm run dev:db && wait-on tcp:5432",
    "dev:server": "dotenvx run --env-file .env.development -- tsx watch --tsconfig tsconfig.node.json server/index.ts",
    "dev:app": "cross-env NODE_ENV=development vite",
    "dev:app:wait": "wait-on http://127.0.0.1:3000/healthz && vite",
    "dev": "npm run dev:db:wait && npm-run-all --parallel dev:server dev:app:wait",
    "build:typecheck": "tsc --build",
    "build:server": "tsx --tsconfig tsconfig.node.json scripts/build-server.ts",
    "build:app": "vite build",
    "build": "npm run build:typecheck && rimraf dist && npm run build:server && npm run build:app",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier . --write",
    "preview": "vite preview",
    "start": "npm run envx:run -- node dist/index.js",
    "db:gen": "dotenvx run --env-file .env.production --env-file .env.development --overload -- drizzle-kit generate",
    "db:mig": "dotenvx run --env-file .env.production --env-file .env.development --overload -- drizzle-kit migrate",
    "prepare": "husky install",
    "etl": "./scripts/run_etl.sh",
    "etl:vessel_departures": "./scripts/run_etl.sh vessel_departures",
    "etl:vessel_arrivals": "./scripts/run_etl.sh vessel_arrivals",
    "etl:vessels_due_to_arrive": "./scripts/run_etl.sh vessels_due_to_arrive"
  }
}
