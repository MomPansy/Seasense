    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "wait-on": "^9.0.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "envx": "dotenvx",
    "envx:run": "npm run envx -- run --env-file .env.production --env-file .env.local --overload",
    "envx:encrypt": "dotenvx encrypt -f .env.production",
    "dev:db": "docker compose up -d",
    "dev:db:wait": "npm run dev:db && wait-on tcp:5432",
    "dev:server": "dotenvx run --env-file .env.development .env.local --overload -- tsx watch --tsconfig tsconfig.node.json server/index.ts",
    "dev:app": "cross-env NODE_ENV=development vite",
    "dev:app:wait": "wait-on http://127.0.0.1:3000/healthz && vite",
    "dev": "npm run dev:db:wait && npm-run-all --parallel dev:server dev:app:wait",
    "build:typecheck": "tsc --build",
    "build:server": "tsx --tsconfig tsconfig.node.json scripts/build-server.ts",
    "build:app": "dotenvx run --env-file .env.vite --env-file .env.production -- vite build",
    "build": "npm run build:typecheck && rimraf dist && npm run build:server && npm run build:app",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier . --write",
    "preview": "vite preview",
    "start": "dotenvx run --env-file .env.vite --env-file .env.production -- node dist/index.js",
    "db:gen": "dotenvx run --env-file .env.production --env-file .env.development --overload -- drizzle-kit generate",
    "db:mig": "dotenvx run --env-file .env.production --env-file .env.development --overload -- drizzle-kit migrate",
    "prepare": "husky install",
    "etl": "./scripts/run_etl.sh",
    "etl:api": "dotenvx run --env-file .env.production --env-file .env.development --overload -- docker compose -f docker-compose.etl.yaml up seasense-etl-api --build",
    "etl:cron": "dotenvx run --env-file .env.production --env-file .env.development --overload -- docker compose -f docker-compose.etl.yaml up seasense-etl-cron --build"
  }
}
